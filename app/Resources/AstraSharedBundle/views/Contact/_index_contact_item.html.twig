{% import "shared_macro.html.twig" as shared %}
<tr>
    <td class="client-avatar">
        {% for alienContact in contact.contactList.contacts %}
            {% if contact!=alienContact %}
                <img alt="image" src="{{ UserPhoto(alienContact.user,'avatar_small') }}">
            {% endif %}
        {% endfor %}
    </td>
    <td><a href="{{ path('astra_contact_view_chat', {'chatId':contact.contactList.id}) }}" class="client-link">{{ contact.fullName }}</a></td>
    <td> Tellus Institute</td>
    <td class="contact-type"><i class="fa fa-envelope"> </i></td>
    <td> gravida@rbisit.com</td>
    <td class="client-status"><span class="label label-primary">Active</span></td>
</tr>